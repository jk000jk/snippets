
//2
#include <stdio.h>

int main()
{
  int i, v[100], j, n, aux;
  printf("n= ");
  scanf("%d", &n);
  
  i = 0;
  j = n - 1;
  for (i = 0; i < n; i++)
	  scanf("%d", &v[i]);
  
  while(i < j)
  {
	while((v[i] % 2 == 0) && (i < n))
		i++;
	
	while((v[j] % 2 == 1) && (j >= 0))
		j--;

	if(i < j) {
		aux = v[i];
		v[i] = v[j];
		v[j] = aux;
	}
  }
  
  for(i = 0; i < n; i++)
	  printf("%d ", v[i]);
	
  return 0;	
}

// 4
#include <stdio.h>

int main()
{
	
	
  int i, v[10], n, aux, d, f[100] = {0};
  printf("n= ");
  scanf("%d", &n);

  for (i = 0; i < n; i++)
	scanf("%d", &v[i]);
  
  for(i = 0; i < n; i++)
  {
	aux = v[i];  
	while(aux >= 10) {	  
		d = aux % 100;
		if(d >= 10) f[d]++;
		aux /= 10;
	}
  }

  for (i = 0; i < 100; i++)
	if(f[i] != 0) printf("f[%d] = %d \n", i, f[i]);  
	
  return 0;	
}



